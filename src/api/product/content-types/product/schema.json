{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "features": {
      "type": "richtext"
    },
    "part_number": {
      "type": "string"
    },
    "warranty_doc": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos",
        "audios",
        "files"
      ]
    },
    "brochure": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "text"
    },
    "brand": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::brand.brand"
    },
    "product_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-category.product-category"
    },
    "product_line": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-line.product-line"
    },
    "dealer_applications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::dealer-application.dealer-application",
      "mappedBy": "products"
    }
  }
}
